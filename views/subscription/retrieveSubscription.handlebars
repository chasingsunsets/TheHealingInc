<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Verified</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        {{#each subscriptions}}
        <tr>
            <td>{{id}}</td>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{email}}</td>
            <td>{{verified}}</td>
            <td>
                <h6 class="delete"><a href="/subscription/deleteSub/{{id}}" data-toggle="confirmation" data-title="Confirm delete?">
                        Delete
                    </a></h6>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>