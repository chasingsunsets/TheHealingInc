{{!-- <div class="container"> --}}
    {{#if quizzes}}
    <div class="card card-body">
        <h3 class="text-primary">Quiz</h3>
        {{#each quizzes}}
        <div class="card border-primary mb-2 w-100 mx-auto">
            <div class="card-body">
                <a href="/quiz/editQuiz/{{id}}" class="card-link mb-2">
                    <i class="fas fa-pencil-alt"></i>
                    Edit
                </a>
                <a href="/quiz/deleteQuiz/{{id}}" class="card-link" data-toggle="con-
                firmation" data-title="Confirm delete?">
                    <i class="far fa-trash-alt"></i>
                    Delete
                </a>
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-3">
                        <p class="font-italic">Name</p>
                        <p class="text-info">
                            {{quizName}}
                        </p>
                    </div>
                    <div class="col-sm-12 col-md-12 col-lg-3">
                        <p class="font-italic">age</p>
                        <p class="text-info">
                            {{age}}
                        </p>
                    </div>
                    <div class="col-sm-12 col-md-12 col-lg-3">
                        <p class="font-italic">supplements</p>
                        <p class="text-info">
                            {{supplements}}
                        </p>
                    </div>
                    <div class="col-sm-12 col-md-12 col-lg-3">
                        <p class="font-italic">health area</p>
                        <p class="text-info">
                            {{area}}
                        </p>
                    </div>
        </div>
        {{/each}}
    </div>
    {{else}}
    <h3>No quiz</h3>
    {{/if}}
</div>